{
  "name": "shortx-sdk",
  "version": "0.1.117",
  "description": "SDK for interacting with the ShortX Protocol Solana contract",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": {
        "types": "./dist/types/cjs/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.cjs"
    },
    "./idl": {
      "require": "./dist/types/idl_shortx.json",
      "import": "./dist/types/idl_shortx.json"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:tsup",
    "build:esm": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:tsup": "tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "solana",
    "anchor",
    "sdk",
    "shortx",
    "protocol"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/shortx-contract.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/shortx-contract/issues"
  },
  "homepage": "https://github.com/yourusername/shortx-contract#readme",
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  }
}
