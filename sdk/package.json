{
  "name": "@endcorp/depredict",
  "version": "0.7.3",
  "description": "SDK for interacting with the Depredict Protocol",
  "type": "module",
  "types": "./dist/cjs/index.d.cts",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "tsc && tsup",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "solana",
    "anchor",
    "sdk",
    "depredict",
    "protocol"
  ],
  "author": "END Corp.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endcorp-hq/depredict.git"
  },
  "bugs": {
    "url": "https://github.com/endcorp-hq/depredict/issues"
  },
  "homepage": "https://github.com/endcorp-hq/depredict#readme",
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@metaplex-foundation/digital-asset-standard-api": "^2.0.0",
    "@metaplex-foundation/mpl-account-compression": "^0.0.1",
    "@metaplex-foundation/mpl-bubblegum": "^5.0.2",
    "@metaplex-foundation/mpl-core": "^1.6.0",
    "@metaplex-foundation/mpl-core-das": "^0.1.0",
    "@metaplex-foundation/mpl-toolbox": "^0.10.0",
    "@metaplex-foundation/umi": "^1.4.1",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@metaplex-foundation/umi-rpc-web3js": "^1.4.1",
    "@metaplex-foundation/umi-web3js-adapters": "^1.2.0",
    "bs58": "^6.0.0"
  }
}
